.bg-white
	background gris !important
	color amarillo !important

	

#menu-off
	background amarillo
	position relative
	width 100%
	& ul ol
		list-style none
		margin 0
		padding 0
	& .content
		list-style none
		margin 0
		padding 0
		& .page-cont
			display inline-block
			margin-left 2em
			margin-bottom -.2em
			& li:hover > ul
				display block
			& li 
				float left
				a
					color gris
					display block
					font-weight bold
					text-decoration none
					padding 1.5em .8em
					text-decoration none
					&:hover
						box-shadow inset 0 -6px 0 0  gris
				& ul
					box-shadow 0px 0px 3px rgba(0, 0, 0, .5)
					color amarillo
					display none
					background gris
					position absolute
					max-width 250px
					margin 0
					padding 0
					transition all 1s ease-in-out
					z-index 999
					&:after
						content: "";
						display: inline-block;
						vertical-align: middle;
						margin-right: 10px;
						width: 0px; 
						height: 0px;
						border:3px solid gris;
						border-right-color:transparent;
						border-left-color:transparent;
						border-width:0px 10px 11px;
						position absolute
						top -.7em
						left 1.1em
						z-index 999
					li
						display block
						list-style none
						width 100%
						&:nth-child(2)
							border-top 1px solid amarillo
							border-bottom 1px solid amarillo
						a
							color amarillo
							list-style none
							display block
							padding .6em .9em
							&:hover
								color gris
								background amarillo
		& .show-actions-user
			float right
			padding .3em 2em
			& figure
				border 2px solid white
				border-radius 50%
				height 40px
				margin 0
				overflow hidden
				width 40px
