#menu-off
	top 0
	left 0
	position absolute
	width 100%
	& ul ol
		list-style none
		margin 0
		padding 0
	& .content
		list-style none
		margin 0
		padding 0
		& .page-cont
			display inline-block
			margin-left 2em
			margin-bottom -.2em
			& li:hover > ul
				display block
			& li
				float left
				a
					color #F9F9F9
					display block
					font-weight normal
					text-decoration none
					padding 1.5em .8em
					font-size 1.1em
					text-decoration none
				& ul
					box-shadow 0px 0px 3px rgba(0, 0, 0, .5)
					color amarillo
					display none
					background gris
					position absolute
					max-width 250px
					margin 0
					padding 0
					transition all 1s ease-in-out
					z-index 999
					&:after
						content: "";
						display: inline-block;
						vertical-align: middle;
						margin-right: 10px;
						width: 0px;
						height: 0px;
						border:3px solid gris;
						border-right-color:transparent;
						border-left-color:transparent;
						border-width:0px 10px 11px;
						position absolute
						top -.7em
						left 1.1em
						z-index 999
					li
						display block
						list-style none
						width 100%
						&:nth-child(2)
							border-top 1px solid amarillo
							border-bottom 1px solid amarillo
						a
							color amarillo
							list-style none
							display block
							padding .6em .9em
							&:hover
								color gris
								background amarillo
		& .show-actions-user
			float right
			padding .3em 2em
			& figure
				height 40px
				margin 0
				width 40px
				& span
					font-size 3em
					color gris
		& .cont-actions-user
			display none
			background gris
			position absolute
			right 2em
			top 4em
			z-index 10000000
			&:after
				content: "";
				display: inline-block;
				vertical-align: middle;
				margin-right: 10px;
				width: 0px;
				height: 0px;
				border:3px solid gris;
				border-right-color:transparent;
				border-left-color:transparent;
				border-width:0px 10px 11px;
				position absolute
				top -.6em
				right .2em
				z-index 1000
			& a:nth-child(2n)
				border-top 1px solid amarillo
				border-bottom 1px solid amarillo
			& a
				color amarillo
				display block
				padding .7em 1em
				text-decoration none
				&:hover
					color gris
					background #FAFAFA
				& li
					list-style none
		& .sessions-cont
