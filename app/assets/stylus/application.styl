verde_p = #90c695
// Colores alertas
verde = #21af7d
rojo = #DC303A

html, body 
    height 100%
    width 100%
    padding 0
    margin 0

body
	background #F3F3F3
	margin 0
	padding 0
	font: 300 16px/120% proxima-nova,sans-serif;
	font-size 16px
	font-family: proxima-nova,sans-serif;

main
	min-height 80%

#alert-success
	background white
	box-shadow 0px 0px 3px verde
	color verde
	font-size 1.1em
	position fixed
	bottom 1em
	left 1em
	padding 1em
	text-align center
	width 300px	
	
#alert-danger
	background white
	box-shadow 0px 0px 3px rojo
	color rojo
	font-size 1.1em
